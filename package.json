{
  "name": "cucumber-js-selenium",
  "version": "1.0.0",
  "description": "UI End 2 End Testing with Cucumber and Selenium",
  "main": "index.js",
  "scripts": {
    "test": "CONFIG_FILE=local-config ./node_modules/.bin/cucumber-js test/features/ --format json",
    "test:local": "CONFIG_FILE=local-config cucumber-js test/features/ -f json:test/report/cucumber_report.json",
    "test:browserstack": "CONFIG_FILE=bs-config cucumber-js test/features/login.feature",
    "report": "node index.js",
    "clean": "tsc --build --clean",
    "build": "tsc --build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sulzer-stuttgart/cucumber-js-selenium.git"
  },
  "keywords": [
    "Cucumber",
    "Selenium",
    "Typescript"
  ],
  "author": "Mustafa Masetic",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/sulzer-stuttgart/cucumber-js-selenium/issues"
  },
  "homepage": "https://github.com/sulzer-stuttgart/cucumber-js-selenium#readme",
  "devDependencies": {
    "chai": "^4.2.0",
    "chromedriver": "^83.0.0",
    "cucumber": "^6.0.5",
    "cucumber-html-reporter": "^5.2.0",
    "selenium-standalone": "^6.17.0",
    "selenium-webdriver": "^4.0.0-alpha.7"
  }
}
